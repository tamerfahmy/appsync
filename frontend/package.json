{
  "name": "city-management",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "npm i -g serverless dynamodb-admin @aws-amplify/cli@4.23.1 && serverless dynamodb install",
    "ng": "ng",
    "start": "ng serve -o",
    "start:proxy": "ng serve -o --proxy-config proxy.conf.json",
    "start:dev": "concurrently -kill-others \"npm run start:proxy\" \"serverless offline start\" \"dynamodb-admin\"",
    "start:proxy:mock:server": "concurrently -kill-others \"npm run mock:server\" \"npm run start:proxy\"",
    "mock:server": "json-server -p 8080 --id trainId src/app/mocks/db.json --routes src/app/mocks/routes.json ",
    "docker:start:dev": "concurrently -kill-others \"ng serve --host 0.0.0.0 --proxy-config proxy.conf.json\" \"serverless offline start --host 0.0.0.0\" \"dynamodb-admin\"",
    "import-schema": "shx cp -r ../appsync/schema.graphql ./schema.graphql",
    "codegen": "npm run import-schema && amplify codegen",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --browsers Chrome --code-coverage",
    "test:ci": "ng test --browsers ChromeHeadless --code-coverage --watch=false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "^13.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@fullcalendar/core": "^5.10.1",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@okta/okta-auth-js": "^6.0.0",
    "angular-gauge": "^4.0.0",
    "angular-oauth2-oidc": "^9.1.1",
    "angular-oauth2-oidc-jwks": "^9.0.0",
    "aws-amplify": "^3.3.25",
    "aws-appsync": "^4.0.3",
    "bootstrap": "^5.0.2",
    "bootstrap-icons": "^1.5.0",
    "chart.js": "^2.9.4",
    "graphql": "^14.7.0",
    "graphql-tag": "^2.11.0",
    "jquery": "^3.5.1",
    "jquery-ui-dist": "^1.12.1",
    "jwt-decode": "^3.1.2",
    "primeicons": "^4.0.0",
    "primeng": "^9.0.6",
    "quill": "^1.3.7",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.10",
    "@angular/cli": "~12.2.10",
    "@angular/compiler-cli": "~12.2.0",
    "@redux-offline/redux-offline": "^2.6.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "concurrently": "^5.3.0",
    "jasmine-core": "~3.8.0",
    "json-server": "^0.15.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "karma-junit-reporter": "^2.0.1",
    "serverless": "^2.30.3",
    "serverless-appsync-plugin": "1.8.0",
    "serverless-appsync-simulator": "0.11.0",
    "serverless-dynamodb-local": "0.2.39",
    "serverless-offline": "6.8.0",
    "serverless-plugin-offline-dynamodb-stream": "1.0.20",
    "shx": "^0.3.4",
    "typescript": "~4.3.5"
  }
}
